-utf8
--backup-and-modify-in-place
--backup-file-extension='/'
--add-trailing-commas
--maximum-line-length=600
--default-tabsize=4
--space-prototype-paren=0
--iterations=8
--space-backslash-quote=2
--keyword-paren-inner-tightness=2
--comma-arrow-breakpoints=0
--paren-tightness=2
--square-bracket-tightness=2
--nospace-for-semicolon
--nospace-terminal-semicolon
--cuddled-paren-brace-weld
--tight-secret-operators
--no-space-keyword-paren
--weld-nested-containers
--break-before-hash-brace=0
--break-before-square-bracket=0
--no-opening-brace-on-new-line
--no-opening-sub-brace-on-new-line
--no-opening-anonymous-sub-brace-on-new-line
--opening-token-right
--stack-opening-tokens
--noblanks-before-comments
-wbb="% + - * / x != == >= <= =~ !~ < > | & >= < = **= += *= &= <<= &&= -= /= |= >>= ||= .= %= ^= x="

